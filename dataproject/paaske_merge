# create dummy for merging later 
import pandas as pd

df = pd.read_pickle('./paaskedage.pkl')

month_dict = { 'april' : '04', 'marts' : '05'}

years = df.year.to_list()
months = df.month.to_list()
months_index = [month_dict[i] for i in months]  

start_date_str = df.start_date.to_list()
start_date = [int(i) for i in start_date_str]

end_date_str = df.end_date.to_list()
end_date = [int(i) for i in end_date_str]


date = []
for year, month, start, end in zip(years,months_index,start_date, end_date):
    dates = [f'{year}-{month}-{str(i).zfill(2)}' for i in range(start, end+1)]
    for the_date in dates:
        date.append(the_date)

df = pd.DataFrame(data = date, columns = ['date'])

df['easter_week'] = 1
df.to_pickle('./data/dates.pkl')

print(df)


